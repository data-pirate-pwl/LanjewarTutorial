<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout android:orientation="vertical"
        android:layout_height="fill_parent"
        android:layout_width="fill_parent"
        tools:ignore="MissingConstraints">

        <LinearLayout android:orientation="vertical"
            android:layout_weight="1"
            android:layout_height="0dp"
            android:layout_width="fill_parent">


            <LinearLayout android:orientation="horizontal"
                android:layout_height="fill_parent"
                android:layout_width="fill_parent">

                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp">
                    <TextView
                        android:singleLine="true"
                        android:background="#C5F1F6"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:textSize="26dp"
                        android:text=" Username : "/>
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp">
                    <TextView
                        android:background="#C5F1F6"
                        android:id="@+id/uname"
                        android:textAlignment="textEnd"
                        android:textSize="26dp"
                        android:textStyle="bold"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="Loading  "/>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
        <LinearLayout android:orientation="vertical"
            android:layout_weight="1"
            android:layout_height="0dp"
            android:layout_width="fill_parent">



            <LinearLayout android:orientation="vertical"
                android:layout_weight="1"
                android:layout_height="0dp"
                android:layout_width="fill_parent">




                <LinearLayout android:orientation="horizontal"
                    android:layout_height="fill_parent"
                    android:layout_width="fill_parent">

                    <LinearLayout
                        android:layout_weight="1"
                        android:layout_height="fill_parent"
                        android:layout_width="0dp">
                        <TextView
                            android:singleLine="true"
                            android:background="#F6CDCD"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="26dp"
                            android:text=" Stu. Name :"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_weight="1"
                        android:layout_height="fill_parent"
                        android:layout_width="0dp">
                        <TextView
                            android:textStyle="bold"
                            android:background="#F6CDCD"
                            android:id="@+id/sname"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="26dp"
                            android:textAlignment="viewEnd"
                            android:text=" Loading  "/>
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>
        </LinearLayout>

        <LinearLayout android:orientation="vertical"
            android:layout_weight="1"
            android:layout_height="0dp"
            android:layout_width="fill_parent">


            <LinearLayout android:orientation="horizontal"
                android:layout_height="fill_parent"
                android:layout_width="fill_parent">

                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp">
                    <TextView
                        android:singleLine="true"
                        android:background="#C5F1F6"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:textSize="26dp"
                        android:text=" Father Name :"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp">
                    <TextView
                        android:background="#C5F1F6"
                        android:id="@+id/fname"
                        android:textAlignment="textEnd"
                        android:textSize="26dp"
                        android:textStyle="bold"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="Loading  "/>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout android:orientation="vertical"
            android:layout_weight="1"
            android:layout_height="0dp"
            android:layout_width="fill_parent">


            <LinearLayout android:orientation="horizontal"
                android:layout_height="fill_parent"
                android:layout_width="fill_parent">

                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp">
                    <TextView
                        android:singleLine="true"
                        android:background="#F6CDCD"
                        android:textSize="26dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text=" Mother Name :"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp">
                    <TextView
                        android:background="#F6CDCD"
                        android:id="@+id/mname"
                        android:textSize="26dp"
                        android:textStyle="bold"
                        android:textAlignment="textEnd"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="Loading "/>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout android:orientation="vertical"
            android:layout_weight="1"
            android:layout_height="0dp"
            android:layout_width="fill_parent">


            <LinearLayout android:orientation="horizontal"
                android:layout_height="fill_parent"
                android:layout_width="fill_parent">

                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp">
                    <TextView
                        android:singleLine="true"
                        android:background="#C5F1F6"
                        android:textSize="26dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text=" Last Name : "/>
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp">
                    <TextView
                        android:background="#C5F1F6"
                        android:textSize="26dp"
                        android:textStyle="bold"
                        android:id="@+id/lname"
                        android:textAlignment="textEnd"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text=" Loading "/>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
        <LinearLayout android:orientation="vertical"
            android:layout_weight="1"
            android:layout_height="0dp"
            android:layout_width="fill_parent">


            <LinearLayout android:orientation="horizontal"
                android:layout_height="fill_parent"
                android:layout_width="fill_parent">

                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp">
                    <TextView
                        android:singleLine="true"
                        android:background="#F6CDCD"
                        android:textSize="26dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text=" Birth Date : "/>
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp">
                    <TextView
                        android:background="#F6CDCD"
                        android:id="@+id/dob"
                        android:textSize="26dp"
                        android:textStyle="bold"
                        android:textAlignment="textEnd"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="Loading "/>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout android:orientation="vertical"
            android:layout_weight="1"
            android:layout_height="0dp"
            android:layout_width="fill_parent">
            <LinearLayout android:orientation="horizontal"
                android:layout_height="fill_parent"
                android:layout_width="fill_parent">

                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp">
                    <TextView
                        android:singleLine="true"
                        android:background="#C5F1F6"
                        android:textSize="26dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text=" UID No : "/>
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp">
                    <TextView
                        android:background="#C5F1F6"
                        android:textSize="26dp"
                        android:textStyle="bold"
                        android:id="@+id/aadhar"
                        android:textAlignment="textEnd"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="Loading "/>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>


        <LinearLayout android:orientation="vertical"
            android:layout_weight="1"
            android:layout_height="0dp"
            android:layout_width="fill_parent">


            <LinearLayout android:orientation="horizontal"
                android:layout_height="fill_parent"
                android:layout_width="fill_parent">

                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp">
                    <TextView

                        android:background="#F6CDCD"
                        android:textSize="26dp"
                        android:singleLine="true"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text=" Gender : "/>
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp">
                    <TextView
                        android:background="#F6CDCD"
                        android:id="@+id/gender"
                        android:textSize="26dp"
                        android:textStyle="bold"
                        android:textAlignment="textEnd"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="Loading "/>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout android:orientation="vertical"
            android:layout_weight="1"
            android:layout_height="0dp"
            android:layout_width="fill_parent">
            <LinearLayout android:orientation="horizontal"
                android:layout_height="fill_parent"
                android:layout_width="fill_parent">

                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp"/>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp">

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:orientation="vertical"
            android:layout_weight="1"
            android:layout_height="0dp"
            android:layout_width="fill_parent">
            <LinearLayout android:orientation="horizontal"
                android:layout_height="fill_parent"
                android:layout_width="fill_parent">

                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp"/>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp">

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:orientation="vertical"
            android:layout_weight="1"
            android:layout_height="0dp"
            android:layout_width="fill_parent">

        </LinearLayout>

        <LinearLayout android:orientation="vertical"
            android:layout_weight="1"
            android:layout_height="0dp"
            android:layout_width="fill_parent">
            <LinearLayout android:orientation="horizontal"
                android:layout_height="fill_parent"
                android:layout_width="fill_parent">

                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp"/>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp">

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout android:orientation="vertical"
            android:layout_weight="1"
            android:layout_height="0dp"
            android:layout_width="fill_parent">
            <LinearLayout android:orientation="horizontal"
                android:layout_height="fill_parent"
                android:layout_width="fill_parent">

                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp"/>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp">

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout android:orientation="vertical"
            android:layout_weight="1"
            android:layout_height="0dp"
            android:layout_width="fill_parent">
            <LinearLayout android:orientation="horizontal"
                android:layout_height="fill_parent"
                android:layout_width="fill_parent">

                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp"/>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_height="fill_parent"
                    android:layout_width="0dp">

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>